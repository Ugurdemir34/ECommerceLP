{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Service": {
    "IAuthRemoteCallService": {
      "Endpoint": "http://localhost/",
      "NamingStrategy": "CamelCase",
      "ContentType": "application/json",
      "RetrySettings": {
        "ConnectionRetryTimes": 4,
        "ConnectionBetweenExceptionWait": "00:00:02",
        "HttpStatusCodes": [],
        "IsEnabled": true
      }
    }

  }
}
